#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/MONISHA/Desktop/13th task manager/node_modules/.pnpm/next@16.0.0_@babel+core@7.2_c1b80d4ff242a171ae83e2e0b4eafdc1/node_modules/next/dist/bin/node_modules:/mnt/c/Users/MONISHA/Desktop/13th task manager/node_modules/.pnpm/next@16.0.0_@babel+core@7.2_c1b80d4ff242a171ae83e2e0b4eafdc1/node_modules/next/dist/node_modules:/mnt/c/Users/MONISHA/Desktop/13th task manager/node_modules/.pnpm/next@16.0.0_@babel+core@7.2_c1b80d4ff242a171ae83e2e0b4eafdc1/node_modules/next/node_modules:/mnt/c/Users/MONISHA/Desktop/13th task manager/node_modules/.pnpm/next@16.0.0_@babel+core@7.2_c1b80d4ff242a171ae83e2e0b4eafdc1/node_modules:/mnt/c/Users/MONISHA/Desktop/13th task manager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/MONISHA/Desktop/13th task manager/node_modules/.pnpm/next@16.0.0_@babel+core@7.2_c1b80d4ff242a171ae83e2e0b4eafdc1/node_modules/next/dist/bin/node_modules:/mnt/c/Users/MONISHA/Desktop/13th task manager/node_modules/.pnpm/next@16.0.0_@babel+core@7.2_c1b80d4ff242a171ae83e2e0b4eafdc1/node_modules/next/dist/node_modules:/mnt/c/Users/MONISHA/Desktop/13th task manager/node_modules/.pnpm/next@16.0.0_@babel+core@7.2_c1b80d4ff242a171ae83e2e0b4eafdc1/node_modules/next/node_modules:/mnt/c/Users/MONISHA/Desktop/13th task manager/node_modules/.pnpm/next@16.0.0_@babel+core@7.2_c1b80d4ff242a171ae83e2e0b4eafdc1/node_modules:/mnt/c/Users/MONISHA/Desktop/13th task manager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../next@16.0.0_@babel+core@7.2_c1b80d4ff242a171ae83e2e0b4eafdc1/node_modules/next/dist/bin/next" "$@"
else
  exec node  "$basedir/../../../../../next@16.0.0_@babel+core@7.2_c1b80d4ff242a171ae83e2e0b4eafdc1/node_modules/next/dist/bin/next" "$@"
fi
